            "specific": []
        },
        "cautions": {
            "common": [
                "Oliguria: discontinue if renal failure occurs (dosage adjustment if reduced creatinine clearance)",
                "Pain occurs at inj site"
            ],
            "specific": [
                {
                    "type": "Do NOT use disodium edetate (Na2H2EDTA) for heavy metal toxicity",
