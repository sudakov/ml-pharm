    "interactions": [
        {
            "classification_type": "Serious",
            "interaction_with": "iobenguane I 131",
            "description": {
                "common": "black cohosh will decrease the level or effect of iobenguane I 131 by  Other (see comment). Avoid or Use Alternate Drug. Based on the mechanism of action of iobenguane, drugs that reduce catecholamine uptake or that deplete catecholamine stores may interfere with iobenguane uptake into cells, and thus, reduce iobenguane efficacy. Discontinue interfering drugs for at least 5 half-lives before administration of either the dosimetry or an iobenguane dose. Do not administer these drugs until at least 7 days after each iobenguane dose."
            }
        },
        {
            "classification_type": "Minor",
            "interaction_with": "nevirapine",
            "description": {
                "common": "nevirapine, black cohosh.\nEither increases toxicity of the other by pharmacodynamic synergism. Minor/Significance Unknown. Increased risk of hepatoxicity."
            }
        },
        {
            "classification_type": "Minor",
            "interaction_with": "tamoxifen",
            "description": {
                "common": "black cohosh increases effects of tamoxifen by pharmacodynamic synergism. Minor/Significance Unknown."
            }
        },
        {
            "classification_type": "Minor",
            "interaction_with": "tenofovir DF",
            "description": {
                "common": "black cohosh, tenofovir DF.\nEither increases toxicity of the other by pharmacodynamic synergism. Minor/Significance Unknown. Increased risk of hepatoxicity."
            }
        },
        {
            "classification_type": "Minor",
            "interaction_with": "zidovudine",
            "description": {
                "common": "black cohosh increases toxicity of zidovudine by pharmacodynamic synergism. Minor/Significance Unknown. Increased risk of hepatoxicity."
            }
        }
    ],
